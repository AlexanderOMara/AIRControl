<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AIRControl" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\AIRControl" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add library="user32" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\AIRControl" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add directory="..\..\..\includes" />
		</Compiler>
		<Linker>
			<Add option="-lwinmm" />
			<Add library=".\FlashRuntimeExtensions.lib" />
		</Linker>
		<Unit filename="main.cpp" />
		<Unit filename="main.h" />
		<Unit filename="resource.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
